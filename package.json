{
  "name": "parcel-starter",
  "version": "0.1.0",
  "author": "Michael Daniel Padilla <michaeldpadilla@gmail.com> (https://mikedpad.com)",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/mikedpad/parcel-starter.git"
  },
  "description": "A starting point for React apps.",
  "scripts": {
    "start": "react-scripts start",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint 'src/**/*.{js,jsx}'",
    "lint:prettier": "prettier --check 'src/**/*.{js,jsx}'",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "polished": "^4.0.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.0.0-beta.2",
    "react-scripts": "^4.0.0",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^3.1.0",
    "prettier": "^2.1.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
