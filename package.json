{
  "name": "parcel-starter",
  "version": "0.1.2",
  "author": "Michael Daniel Padilla <michaeldpadilla@gmail.com> (https://mikedpad.com)",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/mikedpad/parcel-starter.git"
  },
  "description": "A barebones starting point for Javascript development using Parcel Bundler.",
  "main": "./src/index.html",
  "scripts": {
    "start": "npm run dev",
    "dev": "parcel src/index.html",
    "build": "parcel build src/index.html",
    "watch": "parcel watch src/index.html",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint 'src/**/*.{js,jsx}'",
    "lint:prettier": "prettier --check 'src/**/*.{js,jsx}'",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "gh-pages": "^3.1.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.1.2",
    "sass": "^1.29.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
